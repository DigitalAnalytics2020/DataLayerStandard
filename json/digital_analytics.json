  "Root": {
    "name": "Root Object (Initialize)",
    "description": [
      "A data layer JSO is essentially a JavaScript object to collect data about web site that the digital analytics team will extract values from in order to pass into Adobe Launch for further handling. <br />",
      "To add the data layer JSO to your site, insert the following code in the &lt;head&gt; of every page of your website.<br /><br />",
      "&lt;script&gt;",
      "&nbsp;&nbsp;var CWC_MLCA = CWC_MLCA || {};",
      "&nbsp;&nbsp;CWC_MLCA.dataLayer = {};",
      "&nbsp;&nbsp;CWC_MLCA.dataLayer.debug = &quot;true&quot;; // turn on debug information",
      "&lt;/script&gt;"
    ]
  },
  "Site": {
    "name": "Site Object",
    "description": [
      "The site object carries details about the web site and holds following values:<br />",
      " &quot;brand&quot;: company name // not necessary ?",
      " &quot;name&quot;: web site name",
      " &quot;type&quot;: web site type for desktop or mobile // not necessary ?",
      " &quot;environment&quot;: production / staging / development // not necessary ?",
      " &quot;version&quot;: version number",
      " &quot;lastBuildDate&quot;: build date",
      " &quot;province&quot;: like &quot;ON&quot; // remove",
      " &quot;country&quot;: like &quot;CA&quot; // field name also use &ldquo;geoRegion&rdquo;, remove<br /><br />",
      "Sample code:<br />",
      "&lt;script&gt;",
      "var site =",
      "{",
      "&nbsp;&nbsp;&quot;brand&quot;: &quot;Manulife&quot;,",
      "&nbsp;&nbsp;&quot;name&quot;: &quot;Cover Me&quot;,",
      "&nbsp;&nbsp;&quot;type&quot;: &quot;desktop&quot;,",
      "&nbsp;&nbsp;&quot;environment&quot;: &quot;production&quot;,",
      "&nbsp;&nbsp;&quot;version&quot;: &quot;1.0.1&quot;,",
      "&nbsp;&nbsp;&quot;lastBuildDate&quot;: &quot;23/12/2019&quot;, // need standardize format",
      "&nbsp;&nbsp;&quot;province&quot;: &quot;ON&quot;, //",
      "&nbsp;&nbsp;&quot;country&quot;: &quot;CA&quot; // geoRegion",
      "};",
      "CWC_MLCA.dataLayer.site = site;",
      "&lt;/script&gt;"
    ]
  },
  "Page": {
    "name": "Page Object",
    "description": [
      "The page object carries details about each page of web site and holds following values:<br />",
      " &quot;name&quot;: page name",
      " &quot;title&quot;: page title",
      " &quot;category&quot;: //",
      " &quot;language&quot;: language (EN or FR)<br /><br />",
      "Sample code:<br />",
      "&lt;script&gt;",
      "var page =",
      "{",
      "&nbsp;&nbsp;&quot;name&quot;: &quot;covme:health-insurance:critical-illness:support&quot;,",
      "&nbsp;&nbsp;&quot;title&quot;: &quot;CoverMe Health Insurance Support&quot;,",
      "&nbsp;&nbsp;&quot;category&quot;: &quot;&quot;,",
      "&nbsp;&nbsp;&quot;language&quot;: &quot;EN&quot;",
      "};",
      "CWC_MLCA.dataLayer.page = page;",
      "&lt;/script&gt;"
    ]
  },
  "Products": {
    "name": "Product(s) Array Object",
    "description": [
      "The product(s) object carries details about each product of web site and holds following values:<br />",
      " &quot;name&quot;: product name",
      " &quot;productID&quot;: product ID",
      " &quot;category&quot;: product category",
      " &quot;group&quot;: product group",
      " &quot;coverage&quot;: day/time",
      " &quot;value&quot;: $//",
      " &quot;tipInfo&quot;: description<br /><br />",
      "Sample code:<br />",
      "&lt;script&gt;",
      "CWC_MLCA.dataLayer.products = this.dataLayer.products || [];",
      "CWC_MLCA.dataLayer.products.unshift(",
      "{",
      "&nbsp;&nbsp;&quot;name&quot;: &quot;Travel in Canada&quot;,",
      "&nbsp;&nbsp;&quot;productID&quot;: &quot;ti01234567&quot;,",
      "&nbsp;&nbsp;&quot;category&quot;: &quot;travel insurance&quot;, //",
      "&nbsp;&nbsp;&quot;group&quot;: &quot;insurance&quot;, //",
      "&nbsp;&nbsp;&quot;coverage&quot;: &quot;45 days&quot;, //",
      "&nbsp;&nbsp;&quot;value&quot;: &quot;$100&quot;, //",
      "&nbsp;&nbsp;&quot;tipInfo&quot;: &quot;single coverage: 45 days&quot; //description",
      "}",
      ");",
      "&lt;/script&gt;"
    ],
    "demo": {
      "description": "Click link to add the product",
      "sample": [
        "&nbsp;&nbsp;&quot;name&quot;: &quot;Travel in Canada&quot;,",
        "&nbsp;&nbsp;&quot;productID&quot;: &quot;ti01234567&quot;,",
        "&nbsp;&nbsp;&quot;category&quot;: &quot;travel insurance&quot;,",
        "&nbsp;&nbsp;&quot;group&quot;: &quot;insurance&quot;,",
        "&nbsp;&nbsp;&quot;coverage&quot;: &quot;45 days&quot;,",
        "&nbsp;&nbsp;&quot;value&quot;: &quot;$100&quot;,",
        "&nbsp;&nbsp;&quot;tipInfo&quot;: &quot;single coverage: 45 days&quot;"
      ],
      "sampleflag": false
    }
  },
  "Forms": {
    "name": "Application/Forms/Quote/Tool Array Object",
    "description": [
      "The application/forms/quote/tool are object carries details about multiple steps of process and holds following values:<br />",
      " &quot;name&quot;: application/forms/quote/tool name",
      " &quot;appID&quot;: app ID",
      " &quot;step&quot;: 1, 2, 3 … (step number) // separate into 3 groups: start, in-between, complete",
      " &quot;type&quot;: application / quote / tool",
      " &quot;event&quot;: event of each steps needs to push into event object<br /><br />",
      "Sample code:<br />",
      "&lt;script&gt;",
      "CWC_MLCA.dataLayer.apps = [];",
      "CWC_MLCA.dataLayer.apps.push(",
      "{",
      "&nbsp;&nbsp;&quot;name&quot;: &quot;LifeInsuranceCaculate&quot;,",
      "&nbsp;&nbsp;&quot;appID&quot;: &quot;LI01234567&quot;,",
      "&nbsp;&nbsp;&quot;step&quot;: &quot;quote start&quot;, //",
      "&nbsp;&nbsp;&quot;type&quot;: &quot;quote&quot; //",
      "}",
      ");",
      "CWC_MLCA.dataLayer.events.push(&quot;quote:start&quot;); // ?",
      "CWC_MLCA.dataLayer.events.push(",
      "{",
      "&nbsp;&nbsp;&quot;action&quot;: &quot;submit-ti012&quot;,",
      "&nbsp;&nbsp;&quot;category&quot;: &quot;submit&quot;,",
      "&nbsp;&nbsp;&quot;name&quot;: &quot;Travel Insurance Quote&quot;, //",
      "&nbsp;&nbsp;&quot;detail&quot;: &quot;Travel Insurance Prompt 2020&quot; //",
      "}",
      ");",
      "&lt;/script&gt;"
    ],
    "demo": {
      "description": "Click next button to submit each forms",
      "forms": [ "Input value of start step", "Input value of in-between step", "Input value of complete step" ],
      "formindex": 0
    }
  },
  "Events": {
    "name": "Event(s) Array Object",
    "description": [
      "The event(s) object carries details about event (include event history) and holds following values:<br />",
      " &quot;action&quot;: unique event ID different with previous event",
      " &quot;category&quot;: transaction / click / exit / download / redirect / submit / content",
      " &quot;label&quot;: event name",
      " &quot;detail&quot;: like campaign ID related to the event<br /><br />",
      "Sample code:<br />",
      "&lt;script&gt;",
      "CWC_MLCA.dataLayer.actions = CWC_MLCA.dataLayer.actions || [];",
      "CWC_MLCA.dataLayer.actions.push(",
      "&quot;event&quot;: &quot;download&quot;,",
      "&quot;action&quot;: {",
      "&nbsp;&nbsp;&quot;action&quot;: &quot;download-pdf-policy012&quot;,",
      "&nbsp;&nbsp;&quot;category&quot;: &quot;download&quot;, //",
      "&nbsp;&nbsp;&quot;label&quot;: &quot;Policy Booklet Download&quot;, //",
      "&nbsp;&nbsp;&quot;detail&quot;: &quot;Policy Booklet 2020&quot; //detail description",
      "}",
      ");",
      "&lt;/script&gt;<br />",
      "&lt;script&gt;",
      "CWC_MLCA.dataLayer.actions = CWC_MLCA.dataLayer.actions || [];",
      "CWC_MLCA.dataLayer.actions.push(",
      "&quot;event&quot;: &quot;quote-complete&quot;,",
      "&quot;action&quot;: {",
      "&nbsp;&nbsp;&quot;action&quot;: &quot;transaction-pb012&quot;,",
      "&nbsp;&nbsp;&quot;category&quot;: &quot;transaction&quot;, //",
      "&nbsp;&nbsp;&quot;label&quot;: &quot;Money Transfer&quot;, //",
      "&nbsp;&nbsp;&quot;detail&quot;: &quot;Money Transfer Transaction Saving 2020&quot; //detail description",
      "}",
      ");",
      "&lt;/script&gt;<br />",
      "&lt;script&gt;",
      "CWC_MLCA.dataLayer.actions = CWC_MLCA.dataLayer.actions || [];",
      "CWC_MLCA.dataLayer.actions.push(",
      "&quot;event&quot;: &quot;app-step&quot;,",
      "&quot;action&quot;: {",
      "&nbsp;&nbsp;&quot;action&quot;: &quot;application-show012&quot;,",
      "&nbsp;&nbsp;&quot;category&quot;: &quot;application&quot;, //",
      "&nbsp;&nbsp;&quot;label&quot;: &quot;Mortgage Pre-Approval&quot;, //",
      "&nbsp;&nbsp;&quot;detail&quot;: &quot;Mortgage Pre-Approval Prompt 2020&quot; //detail description",
      "}",
      ");",
      "&lt;/script&gt;<br />",
      "&lt;script&gt;",
      "CWC_MLCA.dataLayer.actions = CWC_MLCA.dataLayer.actions || [];",
      "CWC_MLCA.dataLayer.actions.push(",
      "&quot;event&quot;: &quot;event&quot;,",
      "&quot;action&quot;: {",
      "&nbsp;&nbsp;&quot;action&quot;: &quot;sample-event&quot;,",
      "&nbsp;&nbsp;&quot;category&quot;: &quot;event&quot;,",
      "&nbsp;&nbsp;&quot;label&quot;: &quot;Sample Event&quot;,",
      "&nbsp;&nbsp;&quot;detail&quot;: &quot;Sample Event 2020&quot; //detail description",
      "}",
      ");",
      "&lt;/script&gt;"
    ],
    "demo": {
      "description": "Click link to push the event",
      "sample": [
        "&quot;event&quot;: &quot;event&quot;,",
        "&quot;action&quot;: {",
        "&nbsp;&nbsp;&quot;action&quot;: &quot;sample-event&quot;,",
        "&nbsp;&nbsp;&quot;category&quot;: &quot;event&quot;,",
        "&nbsp;&nbsp;&quot;label&quot;: &quot;Sample Event&quot;,",
        "&nbsp;&nbsp;&quot;detail&quot;: &quot;Sample Event 2020&quot; //detail description",
        "}"
      ],
      "sampleflag": false
    }
  },
  "Transaction": {
    "name": "Transaction Object",
    "description": [
      "The transaction object carries details about latest transaction and holds following values:<br />",
      " &quot;transactionID&quot;: transaction ID",
      " &quot;userID&quot;: link to user",
      " &quot;mount&quot;: transaction mount",
      " &quot;category&quot;: transaction product category",
      " &quot;product&quot;: productID / product name",
      " &quot;event&quot;: event of transaction needs to push into event object<br /><br />",
      "Sample code:<br />",
      "&lt;script&gt;",
      "CWC_MLCA.dataLayer.actions = CWC_MLCA.dataLayer.actions || [];",
      "CWC_MLCA.dataLayer.actions.push(",
      "&quot;event&quot;: &quot;transaction&quot;,",
      "&quot;transaction&quot;: {",
      "&nbsp;&nbsp;&quot;transactionID&quot;: &quot;t012&quot;,",
      "&nbsp;&nbsp;&quot;userID&quot;: &quot;user012&quot;,",
      "&nbsp;&nbsp;&quot;mount&quot;: &quot;$120&quot;,",
      "&nbsp;&nbsp;&quot;category&quot;: &quot;transfer&quot;,",
      "&nbsp;&nbsp;&quot;product&quot;: &quot;bank saving&quot;",
      "}",
      ");",
      "&lt;/script&gt;"
    ],
    "demo": {
      "description": "Click link to set transaction and push the event",
      "sample": [
        "&nbsp;&nbsp;<b><i>set transaction</i> :</b>",
        "&nbsp;&nbsp;&quot;transactionID&quot;: &quot;t012&quot;,",
        "&nbsp;&nbsp;&quot;userID&quot;: &quot;user012&quot;,",
        "&nbsp;&nbsp;&quot;mount&quot;: &quot;$120&quot;,",
        "&nbsp;&nbsp;&quot;category&quot;: &quot;transfer&quot;,",
        "&nbsp;&nbsp;&quot;product&quot;: &quot;bank saving&quot;"
